    "random-seed": "^0.3.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "~1.69",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "three": "^0.158.0",
    "typescript": "^5.2.2",
    "vue": "^3.3.8",
    "vue-drag-resize": "^1.5.4",
    "vue-loader": "^17.3.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.15",
    "vue-tilt.js": "^1.1.1",
    "vuetify": "^3.4.0",
    "vuetifyjs-mix-extension": "^0.0.20",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-plugin-vuetify": "^2.0.1"
  },
  "dependencies": {
    "@twurple/api": "^7.0.5",
